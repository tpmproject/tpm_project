<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tpm.comment">
	<insert id="addComment" parameterType="tpm.comment.model.CommentDTO">
		insert into tpm_comment
		values(comment_idx.nextval, #{work_idx}, #{member_idx}, #{comment_content}, sysdate)
	</insert>

</mapper>