<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tpm.project_member">
	<insert id="projectMemberInsert" parameterType="tpm.project.model.ProjectMemberDTO">
		insert into tpm_project_member
		values(
			#{project_idx},
			#{member_idx},
			#{project_level}
			)
	</insert>
	<update id="projectMeberUpdate" parameterType="tpm.project.model.ProjectMemberDTO">
		update tpm_project_member
		set
		project_idx=#{project_idx},
		project_member_idx=#{project_member_idx},
		project_level=#{project_level}
		
	</update>
</mapper>